{
  "name": "phev-mqtt",
  "version": "1.0.0",
  "description": "mqtt wrapper",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit",
    "start": "node index.js",
    "test-unit": "cross-env NODE_ENV=test DEBUG=true mocha --require babel-polyfill --compilers js:babel-core/register --recursive ./src/**/*.spec.js",
    "test-watch": "cross-env NODE_ENV=test DEBUG=true mocha --watch --require babel-polyfill --compilers js:babel-core/register --recursive ./src/**/*.spec.js",
    "build": "babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/papawattu/phev-mqtt.git"
  },
  "author": "Jamie Nuttall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/papawattu/phev-mqtt/issues"
  },
  "homepage": "https://github.com/papawattu/phev-mqtt#readme",
  "dependencies": {
    "mqtt": "^2.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "chai": "^4.1.1",
    "cross-env": "^5.0.5",
    "mocha": "^3.5.0",
    "rxjs": "^5.4.3",
    "sinon": "^3.2.1"
  }
}
